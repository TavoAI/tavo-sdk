{
  "name": "@tavoai/sdk",
  "version": "0.4.2",
  "description": "JavaScript/TypeScript SDK for Tavo AI API",
  "keywords": [
    "tavo",
    "ai",
    "security",
    "api",
    "sdk",
    "typescript",
    "javascript"
  ],
  "homepage": "https://docs.tavoai.net/javascript/",
  "bugs": {
    "url": "https://github.com/TavoAI/tavo-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TavoAI/tavo-sdk.git",
    "directory": "packages/javascript"
  },
  "license": "Apache-2.0",
  "author": "Chris Hamilton <cchamilt@tavoai.net>",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "echo 'Documentation generation not yet configured'",
    "postinstall": "./download_engines.sh",
    "download-engines": "./download_engines.sh"
  },
  "dependencies": {
    "@types/node": "^24.6.0",
    "axios": "^1.6.0",
    "eslint": "^9.36.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.2.3",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "module": "dist/index.mjs",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
